<Window x:Class="CEC_License.MainForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CEC_License"
             mc:Ignorable="d" 
         Height="220" Width="204"
ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen" Title="授權視窗"  >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="btnRegistration" Content="註冊機" Grid.Row="0" HorizontalAlignment="Center"  VerticalAlignment="Center" Width="120" Height="40" Click="button1_Click"/>
        <Button x:Name="button2"  Content="授權資訊" Grid.Row="1" HorizontalAlignment="Center"  VerticalAlignment="Center" Width="120" Height="40" Click="button2_Click"/>
    </Grid>
</Window>
